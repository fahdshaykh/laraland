{
    "eslint.validate": [
        "javascript",
        "javascriptvue",
        "vue"
    ],
    "eslint.format.enable": true,
    "editor.codeActionsOnSave": {
        "source.fixAll.eslint": "explicit"
    },
    "files.autoSave": "afterDelay",
    "window.zoomLevel": 0,
    "workbench.colorTheme": "Umbra",
    "editor.fontFamily": "'Cascadia Code', 'Operator Mono', 'Fira Code', 'Menlo Regular'"
}
